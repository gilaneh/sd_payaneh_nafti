<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- <data> -->

        <record id="contract_registration_sequence" model="ir.sequence">
            <field name="name">Contract Registration Sequence</field>
            <field name="code">sd_payaneh_nafti.contract_registration</field>
<!--            <field name="prefix">COST%(y)s-</field>-->
            <field name="padding">4</field>
            <field eval="True" name="use_date_range"/>
